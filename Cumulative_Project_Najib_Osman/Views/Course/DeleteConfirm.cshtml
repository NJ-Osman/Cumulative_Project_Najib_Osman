@model Cumulative_Project_Najib_Osman.Models.Course
@{
    ViewBag.Title = "DeleteConfirm";
}

<div id="containershow">
    <nav>
        <a class="navlink" href="/Course/Show/@Model.CourseId">Cancel</a>
    </nav>
    <h2>Are you sure you want to delete @Model.CourseName?</h2>
    <p>This action cannot be undone</p>
    <form method="POST" action="/Course/Delete/@Model.CourseId">
        <button class="delbutton" href="">Confirm Delete</button>
    </form>
</div>

