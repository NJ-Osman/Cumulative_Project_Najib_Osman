@model Cumulative_Project_Najib_Osman.Models.Student
@{
    ViewBag.Title = "DeleteConfirm";
}

<div id="containershow">
    <nav>
        <a class="navlink" href="/Student/Show/@Model.StudentId">Cancel</a>
    </nav>
    <h2>Are you sure you want to delete @Model.StudentFname @Model.StudentLname?</h2>
    <p>This action cannot be undone</p>
    <form method="POST" action="/Student/Delete/@Model.StudentId">
        <button class="delbutton" href="">Confirm Delete</button>
    </form>
</div>

